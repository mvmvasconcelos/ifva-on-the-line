# Variables for the React Frontend
# Copy this file to .env and fill in your values

# Password hash (SHA-256) for admin access to settings
# Generate with: echo -n "your_password" | sha256sum
VITE_ADMIN_PASSWORD_HASH=your_sha256_hash_here

# GitHub repository for API calls (format: owner/repo)
VITE_GITHUB_REPO=your-username/ifva-on-the-line

# GitHub Personal Access Token (only needed for save/test features)
# This is optional - the app works in read-only mode without it
VITE_GITHUB_TOKEN=your_github_pat_here
